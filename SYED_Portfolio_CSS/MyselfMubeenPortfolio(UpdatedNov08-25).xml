    width: 200px; /* Set a width for the list */
}

/* Show the project list when hovering over the nav-item */
.nav-item:hover .project-list {
    display: block; /* Display the list */
}

/* Styling for the list items */
.project-list ul {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

/* Styling for individual list items */
.project-list li {
    padding: 8px;
    cursor: pointer;
    transition: background-color 0.3s ease; /* Smooth transition */
    opacity: 0; /* Initially hidden */
    transform: translateY(10px); /* Slightly shifted downward */
    animation: fadeIn 0.3s forwards; /* Animation for fading in */
}

/* No background color for individual items */
.project-list li:hover {
    background-color: transparent; /* Remove background on hover */
}

/* Animation to fade and slide the items in */
@keyframes fadeIn {
    0% {
        opacity: 0;
        transform: translateY(10px);
    }
    100% {
        opacity: 1;
        transform: translateY(0);
    }
}

/* Create a delay effect for each list item */
.project-list li:nth-child(1) {
    animation-delay: 0.1s; /* Delay for the first item */
}

.project-list li:nth-child(2) {
    animation-delay: 0.2s; /* Delay for the second item */
}

.project-list li:nth-child(3) {
    animation-delay: 0.3s; /* Delay for the third item */
}

/* Optional: Ensure the list is aligned better */
.nav-item {
    position: relative;
}

          
          
          
          
          
          
          
          
          
          
          
          
          
          
          
          
          
          
          
          









          
          
          
          




          
          
      
