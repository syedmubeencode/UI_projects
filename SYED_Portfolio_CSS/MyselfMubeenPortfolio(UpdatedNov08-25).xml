/* Remove the white border from the dropdown */
.project-list {
    display: none; /* Initially hidden */
    position: absolute; 
    top: 100%; 
    left: 0;
    background-color: transparent; /* No background for the dropdown */
    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);
